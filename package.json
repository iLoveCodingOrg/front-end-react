{
  "name": "ilc-react",
  "version": "1.9.1",
  "description": "iLoveCoding Front-End React Application",
  "main": "src/index.js",
  "repository": {
    "url": "git@bitbucket.org:ilovecodinginc/ilc-react.git",
    "type": "git"
  },
  "scripts": {
    "screenshot": "phantomjs screenshot.js",
    "start": "API_URL=http://localhost:3333/api/ ./node_modules/parcel/bin/cli.js --no-cache ./src/index.html",
    "start-prod": "API_URL=https://api.ilovecoding.org/api/ ./node_modules/parcel/bin/cli.js --no-cache ./src/index.html",
    "prebuild": "rm -rf build",
    "build": "API_URL=https://api.ilovecoding.org/api/ ./node_modules/parcel/bin/cli.js build --no-cache --no-source-maps --public-url=/ --out-dir=build ./src/index.html",
    "postbuild": "cp _copyConfigs/_redirects build/_redirects && cp _copyConfigs/robots.txt build/robots.txt && cp _copyConfigs/sitemap-static.xml build/sitemap-static.xml && cp _copyConfigs/sitemap-courses.xml build/sitemap-courses.xml && cp _copyConfigs/sitemap-blogs.xml build/sitemap-blogs.xml"
  },
  "keywords": [
    "iLoveCoding",
    "react",
    "LMS"
  ],
  "author": "Aziz Ali, @ilovecodingOrg",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "@u-wave/react-vimeo": "^0.5.0",
    "bootstrap": "^4.1.1",
    "braintree-web-drop-in-react": "^1.0.5",
    "lodash": "^4.17.11",
    "open-iconic": "^1.1.1",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "raven-for-redux": "^1.4.0",
    "react": "^16.8.6",
    "react-countdown-now": "^1.3.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-recaptcha": "^2.3.10",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-toastify": "^4.4.3",
    "react-trumbowyg": "^1.1.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "store": "^2.0.12",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "parcel": "^1.12.3",
    "sass": "^1.15.1"
  }
}
