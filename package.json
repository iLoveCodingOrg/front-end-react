{
  "name": "ilc-react",
  "version": "1.0.12",
  "description": "iLoveCoding Front-End React Application",
  "main": "src/index.js",
  "repository": {
    "url": "git@bitbucket.org:ilovecodinginc/ilc-react.git",
    "type": "git"
  },
  "scripts": {
    "screenshot": "phantomjs screenshot.js",
    "start": "API_URL=http://localhost:3333/api/ ./node_modules/parcel/bin/cli.js --no-cache ./src/index.html",
    "prebuild": "rm -rf build",
    "build": "API_URL=https://api.ilovecoding.org/api/ ./node_modules/parcel/bin/cli.js build --no-cache --no-source-maps --no-minify --public-url=/ --out-dir=build ./src/index.html && cp _redirects build/_redirects && cp sitemap.xml build/sitemap.xml"
  },
  "keywords": [
    "iLoveCoding",
