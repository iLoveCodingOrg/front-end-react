    "open-iconic": "^1.1.1",
    "prop-types": "^15.6.2",
    "qs": "^6.5.2",
    "raven-for-redux": "^1.3.1",
    "react": "^16.4.1",
    "react-countdown-now": "^1.3.0",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
